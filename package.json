{
  "name": "next-multilingual",
  "description": "An opinionated end-to-end multilingual solution for Next.js.",
  "version": "0.0.1",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./config": "./lib/config/index.js",
    "./link": "./lib/link/index.js",
    "./link/ssr": "./lib/link/ssr.js",
    "./head": "./lib/head/index.js",
    "./messages": "./lib/messages/index.js",
    "./messages/babel-plugin": "./lib/messages/babel-plugin.js",
    "./messages/properties": "./lib/messages/properties.js"
  },
  "typesVersions": {
    "*": {
      "config": [
        "lib/config/index.d.ts"
      ],
      "link": [
        "lib/link/index.d.ts",
        "lib/link/ssr.d.ts"
      ],
      "head": [
        "lib/head/index.d.ts"
      ],
      "messages": [
        "lib/messages/index.d.ts",
        "lib/messages/babel-plugin.d.ts",
        "lib/messages/properties.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "type": "commonjs",
  "engines": {
    "node": ">=13"
  },
  "watch": {
    "build": {
      "patterns": "*",
      "extensions": "*.ts?(x)",
      "ignore": "node_modules"
    }
  },
  "scripts": {
    "build_watch": "npm-watch",
    "build": "rm -Rf ./lib && tsc",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "chokidar": "^3.5.1",
    "dot-properties": "^1.0.1",
    "nookies": "^2.5.2",
    "resolve-accept-language": "^1.0.25"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@types/babel__core": "^7.1.15",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "next": "^11.1.0",
    "npm-watch": "^0.11.0",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
